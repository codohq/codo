{
  "name": "codohq/codo",
  "description": "Codo framework for PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Anomalyce",
      "homepage": "https://github.com/anomalyce"
    },
    {
      "name": "Xhika",
      "homepage": "https://github.com/xhika"
    }
  ],
  "require": {
    "php": ">=8.1",
    "laravel/framework": "~9"
  },
  "minimum-stability": "dev",
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Codohq\\Codo\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
